{"ast":null,"code":"import DevisInfo from \"../Devis/DevisInfo.vue\";\nexport default {\n  name: \"DashboardBlock\",\n  props: {\n    msg: String\n  },\n  components: {\n    DevisInfo\n  },\n\n  data() {\n    return {};\n  },\n\n  methods: {\n    getStatus(value) {\n      if (value == \"DRAFT\") {\n        let res = \"Brouillon\";\n        value = res;\n      } else if (value == \"CREATED\") {\n        let res = \"Créé\";\n        value = res;\n      } else if (value == \"PAYED\") {\n        let res = \"Facturé\";\n        value = res;\n      } else if (value == \"CANCELLED\") {\n        let res = \"Annulé\";\n        value = res;\n      }\n\n      return value;\n    },\n\n    getStatusButtonClass(value) {\n      if (value == \"DRAFT\") {\n        return 'btn-draft';\n      } else if (value == \"CREATED\") {\n        return 'btn-created';\n      } else if (value == \"PAYED\") {\n        return 'btn-payed';\n      } else if (value == \"CANCELLED\") {\n        return 'btn-cancelled';\n      }\n\n      return 'btn-default';\n    }\n\n  },\n\n  mounted() {\n    this.$store.dispatch(\"getUser\");\n    this.$store.dispatch(\"getDevis\");\n  },\n\n  updated() {},\n\n  computed: {\n    getDevis() {\n      return this.$store.state.Devis.filter((el, inx) => inx < 5);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8FA,OAAOA,SAAP,MAAsB,wBAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,gBADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAFM;EAKbC,UAAU,EAAE;IACVL;EADU,CALC;;EAQbM,IAAI,GAAG;IACL,OAAO,EAAP;EACD,CAVY;;EAYbC,OAAO,EAAE;IACPC,SAAS,CAACC,KAAD,EAAQ;MACf,IAAIA,KAAI,IAAK,OAAb,EAAsB;QACpB,IAAIC,GAAE,GAAI,WAAV;QACAD,KAAI,GAAIC,GAAR;MACF,CAHA,MAGO,IAAID,KAAI,IAAK,SAAb,EAAwB;QAC7B,IAAIC,GAAE,GAAI,MAAV;QACAD,KAAI,GAAIC,GAAR;MACF,CAHO,MAGA,IAAID,KAAI,IAAK,OAAb,EAAsB;QAC3B,IAAIC,GAAE,GAAI,SAAV;QACAD,KAAI,GAAIC,GAAR;MACF,CAHO,MAGA,IAAID,KAAI,IAAK,WAAb,EAA0B;QAC/B,IAAIC,GAAE,GAAI,QAAV;QACAD,KAAI,GAAIC,GAAR;MACF;;MACA,OAAOD,KAAP;IACD,CAhBM;;IAkBRE,oBAAoB,CAACF,KAAD,EAAQ;MACzB,IAAIA,KAAI,IAAK,OAAb,EAAs